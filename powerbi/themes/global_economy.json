{
  "name": "Global Economy Dashboard Theme",

  "dataColors": [
    "#1f77b4",
    "#ff7f0e",
    "#2ca02c",
    "#d62728",
    "#9467bd",
    "#8c564b",
    "#e377c2"
  ],

  "background": "#FFFFFF",
  "foreground": "#000000",
  "tableAccent": "#1f77b4",

  "visualStyles": {
    "*": {
      "*": {
        "title": [
          {
            "show": true,
            "fontSize": 13,
            "fontFace": "Segoe UI Semibold",
            "color": { "solid": { "color": "#000000" } }
          }
        ],
        "legend": [
          {
            "showTitle": false,
            "position": "TopCenter",
            "fontFace": "Segoe UI",
            "fontSize": 10,
            "color": { "solid": { "color": "#000000" } }
          }
        ],
        "categoryAxis": [
          {
            "showAxisTitle": false,
            "gridlineShow": true,
            "gridlineColor": { "solid": { "color": "#e0e0e0" } },
            "labelColor": { "solid": { "color": "#333333" } },
            "fontSize": 10
          }
        ],
        "valueAxis": [
          {
            "showAxisTitle": false,
            "gridlineShow": true,
            "gridlineColor": { "solid": { "color": "#e0e0e0" } },
            "labelColor": { "solid": { "color": "#333333" } },
            "fontSize": 10
          }
        ]
      }
    },

    "card": {
      "*": {
        "categoryLabels": [
          {
            "color": { "solid": { "color": "#444444" } },
            "fontSize": 11,
            "fontFace": "Segoe UI"
          }
        ],
        "labels": [
          {
            "color": { "solid": { "color": "#000000" } },
            "fontSize": 20,
            "fontFace": "Segoe UI Bold"
          }
        ]
      }
    },

    "tableEx": {
      "*": {
        "grid": [
          {
            "gridHorizontal": true,
            "outlineColor": { "solid": { "color": "#e0e0e0" } },
            "outlineWeight": 0.5,
            "rowPadding": 4
          }
        ],
        "columnHeaders": [
          {
            "backColor": { "solid": { "color": "#f5f5f5" } },
            "fontFace": "Segoe UI Semibold",
            "fontSize": 11,
            "color": { "solid": { "color": "#000000" } }
          }
        ],
        "values": [
          {
            "fontFace": "Segoe UI",
            "fontSize": 10,
            "color": { "solid": { "color": "#222222" } }
          }
        ]
      }
    },

    "stackedAreaChart": {
      "*": {
        "legend": [
          {
            "position": "Right",
            "fontSize": 9,
            "fontFace": "Segoe UI"
          }
        ],
        "categoryAxis": [
          {
            "fontSize": 9,
            "labelColor": { "solid": { "color": "#333333" } }
          }
        ],
        "valueAxis": [
          {
            "fontSize": 9,
            "labelColor": { "solid": { "color": "#333333" } }
          }
        ]
      }
    }
  }
}
